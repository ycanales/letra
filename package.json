{
  "name": "Letra",
  "version": "1.0.0",
  "description": "Lyrics from current playing song in Spotify",
  "main": "src/main.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "electron .",
    "dist": "build --mac --x64"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ycanales/spotify-genius.git"
  },
  "keywords": [
    "letra",
    "mac",
    "lyrics",
    "music"
  ],
  "author": "Cristian Yáñez",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/ycanales/letra/issues"
  },
  "homepage": "https://github.com/ycanales/letra",
  "devDependencies": {
    "electron-builder": "^10.17.3",
    "electron-prebuilt": "^1.2.0"
  },
  "dependencies": {
    "angular": "^1.5.8",
    "angular-animate": "^1.5.10",
    "angular-aria": "^1.5.9",
    "angular-material": "^1.1.1",
    "cheerio": "^0.20.0",
    "material-design-icons": "^3.0.1",
    "shelljs": "^0.7.0",
    "tmp": "0.0.31"
  },
  "build": {
    "appId": "com.ycanales.letra",
    "mac": {
      "category": "public.app-category.entertainment"
    },
    "dmg": {
      "title": "Letra for Mac"
    },
    "files": [
      "src/**/*",
      "node_modules/**/*",
      "package.json"
    ]
  }
}
